{
	"version": "2.0.0",
	"configurations": [
		{
			"name": "GCC Premake Sandbox",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/Debug-windows-x86_64/Sandbox/Sandbox.exe",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Premake: Build Projects"
		},
		{
			"name": "GCC CMake Sandbox",
