{
	"version": "2.0.0",
	"configurations": [
		{
			"name": "MSVC Premake Sandbox (Debug)",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/Debug-windows-x86_64/vs2022/Sandbox/Sandbox.exe",
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal",
			"preLaunchTask": "MSVC Premake: Build Projects (Debug)"
		},
		{
			"name": "MSVC Premake Sandbox (Release)",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/Release-windows-x86_64/vs2022/Sandbox/Sandbox.exe",
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal",
			"preLaunchTask": "MSVC Premake: Build Projects (Release)"
		},
		{
			"name": "MSVC Premake Sandbox (Dist)",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/Dist-windows-x86_64/vs2022/Sandbox/Sandbox.exe",
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal",
			"preLaunchTask": "MSVC Premake: Build Projects (Dist)"
		},
		{
			"name": "GCC Premake Sandbox (Debug)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/Debug-windows-x86_64/gmake2/Sandbox/Sandbox.exe",
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "GCC Premake: Build Projects (Debug)"
		},
		{
			"name": "GCC Premake Sandbox (Release)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/Release-windows-x86_64/gmake2/Sandbox/Sandbox.exe",
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "GCC Premake: Build Projects (Release)"
		},
		{
			"name": "GCC Premake Sandbox (Dist)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/Dist-windows-x86_64/gmake2/Sandbox/Sandbox.exe",
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "GCC Premake: Build Projects (Dist)"
		},
		{
			"name": "MSVC CMake Sandbox (Select Variant Before)",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/Debug-Windows-64/MSVC/Sandbox/Sandbox.exe",
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal",
			"preLaunchTask": "CMake: Build"
		},
		{
			"name": "GCC CMake Sandbox (Select Variant Before)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/bin/Debug-Windows-64/GNU/Sandbox/Sandbox.exe",
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "CMake: Build"
		}
	]
}
